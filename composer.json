{
    "name": "spiritix/lada-cache",
    "type": "library",
    "description": "A Redis based, automated and scalable database caching layer for Laravel",
    "license": "MIT",
    "keywords": [
        "php",
        "laravel",
        "cache",
        "caching",
        "redis",
        "database",
        "eloquent",
        "model"
    ],
    "authors": [
        {
            "name": "Matthias Isler",
            "email": "mi@matthias-isler.ch"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/redis": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^10.6",
        "mockery/mockery": "^1.6",
        "barryvdh/laravel-debugbar": "^3.16",
        "predis/predis": "^2.2",
        "laravel/pint": "^1.25",
        "phpstan/phpstan": "^2.1"
    },
    "suggest": {
        "barryvdh/laravel-debugbar": "Provides debug information about the cache"
    },
    "autoload": {
        "psr-4": {
            "Spiritix\\LadaCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spiritix\\LadaCache\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --configuration phpunit.xml --coverage-clover coverage.xml",
        "bugs": "vendor/bin/phpstan analyse --level=max src/",
        "format": "vendor/bin/pint src/"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Spiritix\\LadaCache\\LadaCacheServiceProvider"
            ]
        }
    }
}
